function palindrome(word){
   let usual =word;
//แยก string ลงใน array ของ string แล้ว reverse แล้ว join ใหม่
  let rever= word.split("").reverse().join("") ;
  
  let allResult=[];
  //console.log("usual:",usual);
 // console.log("reverse", rever);
  if(usual === rever){
    return true;
  }else{return false;}  
}

console.log("palindrome:",palindrome("mem"));
console.log("palindrome:",palindrome("memm"));
